{
    "name": "rsuite-color-picker",
    "version": "0.1.0-alpha",
    "description": "Package a color picker extension that can select color for rsuite.",
    "main": "lib",
    "scripts": {
        "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3100 --host 0.0.0.0 ",
        "build": "rm -rf lib && babel src --out-dir lib ",
        "docs": "rm -rf assets &&  NODE_ENV=production webpack",
        "publish-docs": "node docs/gh-pages.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cXiaof/rsuite-color-picker.git"
    },
    "files": [
        "CHANGELOG.md",
        "lib",
        "src"
    ],
    "keywords": [
        "colorpicker",
        "react-color",
        "react",
        "rsuite"
    ],
    "author": "henry.xiaofengchen@qq.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/cXiaof/rsuite-color-picker/issues"
    },
    "homepage": "https://github.com/cXiaof/rsuite-color-picker#readme",
    "peerDependencies": {
        "react": "^16.9.0",
        "rsuite": "^4.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.7.6",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "css-loader": "^2.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^2.29.0",
        "less": "^3.9.0",
        "less-loader": "^4.0.5",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-hot-loader": "^3.1.3",
        "rsuite": "^4.0.0",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.0.0",
        "webpack-dev-server": "^2.3.0"
    },
    "dependencies": {
        "react-color": "^2.17.3"
    }
